{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenRouter": {
    "TimeoutSeconds": 120
  },
  "UserServiceUrl": "http://localhost:5200",
  "PortfolioService": {
    "BaseUrl": "http://localhost:5201"
  },
  "FrontendUrls": [
    "http://localhost:3000",
    "http://localhost:3001",
    "http://localhost:3002",
    "http://localhost:3003"
  ],
  "HttpClient": {
    "UserAgent": "AIService/1.0",
    "HandlerLifetime": "00:05:00",
    "AIProviderTimeout": "00:02:00",
    "AIProviderMaxConnections": 5,
    "PortfolioServiceTimeout": "00:00:30",
    "PortfolioServiceMaxConnections": 10,
    "UserServiceTimeout": "00:00:10",
    "UserServiceMaxConnections": 10
  }
}
